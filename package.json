{
  "name": "mini-modules",
  "version": "1.0.0",
  "private": true,
  "author": "EastonShay",
  "description": "h5与小程序公共模块提取的项目",
  "scripts": {
    "dev": "concurrently \"pnpm run dev:demo\" \"pnpm run dev:doc\"", 
    "dev:bussiness": "pnpm --filter=./bussiness dev",
    "dev:demo": "pnpm --filter=./demo dev:h5",
    "dev:doc": "pnpm --filter=./docs dev",
    "build": "concurrently \"pnpm run build:demo\" \"pnpm run build:doc\"",
    "build:bussiness": "pnpm --filter=./bussiness build",
    "build:demo": "pnpm --filter=./demo build:h5",
    "build:doc": "pnpm --filter=./docs build",
    "clean": " rimraf **/dist && rimraf **/node_modules",
    "version": "run-s version:*",
    "version:release": "pnpm --parallel -r --aggregate-output --filter=./packages/* exec npm version ${npm_package_version}",
    "version:git": "git add . && git commit -m \"chore(release): publish ${npm_package_version}\"",
    "version:changelog": "conventional-changelog -p angular"
  },
  
  "dependencies": {
    "@babel/eslint-parser": "^7.22.15"
  },
  "devDependencies": {
    "@babel/core": "^7.24.4",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.22.10",
    "@babel/preset-react": "^7.24.1",
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@types/node": "^18",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "autoprefixer": "9.8.8",
    "babel-preset-taro": "4.0.8",
    "conventional-changelog-cli": "^2.2.2",
    "cross-env": "^7.0.3",
    "eslint-config-taro": "4.0.8",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-config-o2team": "^0.1.7",
    "eslint-import-resolver-custom-alias": "^1.3.2",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^3.0.2",
    "stylelint": "^16.4.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-scss": "^3.21.0",
    "tslib": "2.5.0",
    "typescript": "^5.4.5"
  }
}

